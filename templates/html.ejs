<!DOCTYPE html>
<html>
    <head>
        <title>Documentation</title>
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.6/readable/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    </head>
    <body>
        <% schema.forEach(function(call) { %>
        <div class="call">
            <h1><%= call.remoteName %></h1>
            <div><%= call.description %></div>
            <ul>
                <% call.routes.forEach(function(route) { %>
                <li><%= route.first().toUpperCase() + " " + route.last() %></li>
                <% }) %>
            </ul>
            <% Object.keys(call.interface).forEach(function(name) { %>
            <div>
                <b><%= name %></b>
            </div>
            <% }) %>
        </div>
        <% }) %>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    </body>
</html>